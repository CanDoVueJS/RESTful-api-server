{
  "name": "hot-place-share-api",
  "version": "0.0.1",
  "description": "CanDoVueJS의 맛집 공유 샘플서비스 Restful API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src",
    "db:init": "node_modules/.bin/sequelize init",
    "db:emptymigration": "node_modules/.bin/sequelize migration:create",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:migrate:undoall": "node_modules/.bin/sequelize db:migrate:undo:all",
    "db:seed": "node_modules/.bin/sequelize db:seed:all",
    "db:seed:undo:all": "node_modules/.bin/sequelize db:seed:undo:all"
  },
  "repository": "CanDoVueJS/hot-place-share-api",
  "author": "Evan Moon <bboydart91@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "morgan": "^1.8.0",
    "sequelize": "^4.41.0",
    "sequelize-cli": "^5.2.0",
    "sqlite3": "^4.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^5.8.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "^1.9.2"
  }
}
