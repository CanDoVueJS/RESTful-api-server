{
  "name": "hot-place-share-api",
  "version": "0.0.1",
  "description": "CanDoVueJS의 맛집 공유 샘플서비스 Restful API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "restart": "kill-port 8000",
    "prestart": "npm run -s build",
    "test": "eslint src",
    "db:init": "npx sequelize init",
    "db:emptymigration": "npx sequelize migration:create",
    "db:migrate": "npx sequelize db:migrate",
    "db:migrate:undoall": "npx sequelize db:migrate:undo:all",
    "db:seed": "npx sequelize db:seed:all",
    "db:seed:undo:all": "npx sequelize db:seed:undo:all",
    "db:migrate:seed": "npm run db:migrate && npm run db:seed",
    "makemigration": "npx makemigration"
  },
  "repository": "CanDoVueJS/hot-place-share-api",
  "author": "Evan Moon <bboydart91@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.13.3",
    "composable-middleware": "^0.3.0",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "date-fns": "^1.30.1",
    "express": "^4.13.3",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.3.0",
    "kill-port": "^1.3.2",
    "morgan": "^1.8.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "sequelize": "^4.41.0",
    "sequelize-cli": "^5.2.0",
    "sqlite3": "^4.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^5.8.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "^1.18.11",
    "sequelize-auto-migrations": "^1.0.3"
  }
}
